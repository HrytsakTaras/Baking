@model IEnumerable<Baking.Data.Entity.User>

@{
    ViewData["Title"] = "Users";
}

<h1>Index</h1>

<table class="table">
    <thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Email)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.IsRegularClient)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Balance)
			</th>
			<th></th>
		</tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.Email)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.IsRegularClient)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Balance)
			</td>
		</tr>
}
    </tbody>
</table>
